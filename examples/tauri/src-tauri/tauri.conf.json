{
  "identifier": "com.simonbourne.silkenweb",
  "plugins": {},
  "build": {
    "devUrl": "http://localhost:8080",
    "frontendDist": "../frontend/dist",
    "beforeDevCommand": "cd frontend && trunk serve",
    "beforeBuildCommand": "cd frontend && trunk build"
  },
  "productName": "silkenweb-tauri-example",
  "mainBinaryName": "silkenweb-tauri-example",
  "version": "0.1.0",
  "app": {
    "security": {
      "csp": null
    },
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "SilkenWeb Tauri Example",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "useHttpsScheme": true
      }
    ]
  }
}